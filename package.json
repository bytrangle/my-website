{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy": "gatsby build && surge public/"
  },
  "dependencies": {
    "@deckdeckgo/highlight-code": "^2.1.1",
    "@mdx-js/mdx": "^1.6.16",
    "@mdx-js/react": "^1.6.16",
    "@svgr/webpack": "^5.4.0",
    "fontsource-raleway": "^3.0.9",
    "gatsby": "^2.20.12",
    "gatsby-image": "^2.3.2",
    "gatsby-plugin-mdx": "^1.2.39",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-react-helmet-canonical-urls": "^1.4.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-plugin-scroll-reveal": "0.0.7",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-remark-copy-linked-files": "^2.3.14",
    "gatsby-remark-highlight-code": "^2.1.0",
    "gatsby-remark-images": "^3.2.5",
    "gatsby-remark-interactive-gifs": "^1.0.3",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-source-wordpress": "^3.2.3",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.4",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "typeface-archivo": "^1.1.13",
    "typeface-archivo-narrow": "^1.1.13",
    "typeface-inconsolata": "0.0.72"
  },
  "devDependencies": {
    "git-scripts": "^0.4.3",
    "prettier": "^1.19.1",
    "sass": "^1.26.10",
    "surge": "^0.21.6",
    "typescript": "^3.8.3"
  },
  "git": {
    "scripts": {
      "pre-push": "gatsby build && surge --project ./public"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
